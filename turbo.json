{
  "$schema": "https://turborepo.com/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "lint": {
      "dependsOn": []
    },
    "test": {
      "dependsOn": []
    },
    "bench": {
      "dependsOn": ["test"],
      "cache": false
    },
    "bench:regress": {
      "dependsOn": ["test"],
      "cache": false
    }
  }
}

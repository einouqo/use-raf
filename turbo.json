{
  "$schema": "https://turborepo.com/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "lint": {
      "dependsOn": []
    },
    "test": {
      "dependsOn": []
    },
    "publish": {
      "dependsOn": ["build", "^publish"]
    }
  }
}
